{
  "version": "1.0",
  "components": [
    {
      "name": "load_excel_data",
      "type": "function",
      "function": "load_excel_data_component",
      "inputs": ["excel_file"],
      "outputs": ["df"]
    },
    {
      "name": "load_template",
      "type": "function",
      "function": "load_template_component",
      "inputs": ["template_file"],
      "outputs": ["template"]
    },
    {
      "name": "replace_fields",
      "type": "function",
      "function": "replace_fields_component",
      "inputs": ["template", "data_row"],
      "outputs": ["report_doc"]
    },
    {
      "name": "generate_reports",
      "type": "function",
      "function": "generate_reports_component",
      "inputs": ["df", "excel_file", "template_file"],
      "outputs": ["result"]
    },
    {
      "name": "main",
      "type": "function",
      "function": "main_component",
      "inputs": [],
      "outputs": []
    }
  ],
  "connections": [
    {
      "from": "main",
      "to": "load_excel_data",
      "input": "excel_file"
    },
    {
      "from": "main",
      "to": "load_template",
      "input": "template_file"
    },
    {
      "from": "load_excel_data",
      "to": "generate_reports",
      "input": "df"
    },
    {
      "from": "load_template",
      "to": "generate_reports",
      "input": "template_file"
    },
    {
      "from": "load_template",
      "to": "replace_fields",
      "input": "template"
    },
    {
      "from": "load_excel_data",
      "to": "replace_fields",
      "input": "data_row"
    },
    {
      "from": "replace_fields",
      "to": "generate_reports",
      "input": "report_doc"
    }
  ]
}
